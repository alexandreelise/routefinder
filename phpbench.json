{
  "$schema": "./vendor/phpbench/phpbench/phpbench.schema.json",
  "runner.bootstrap": "bootstrap.php",
  "runner.file_pattern": "*Bench.php",
  "report.generators": {
    "consumation_of_time": {
      "extends": "default",
      "title": "The Consumation of Time",
      "description": "Benchmark how long it takes to consume time",
      "cols": [
        "subject",
        "mode"
      ]
    }
  },
  "runner.php_binary": "/usr/bin/php8.3",
  "runner.php_disable_ini": true,
  "runner.revs": 1000,
  "runner.iterations": 5
}
